<div id="moveTags" >
    <ul class="nav nav-tabs" role="tablist">
        <% for(var i in tags){ %>
        
        <%  var val = tags[i],active = i==0?'active':''; %>
            <li class=<%= active %>  ><a href='<%=  "#"+val.id %>' data-toggle="tab" ><%= val.value %></a></li>
        <% } %>
    </ul>
    <div class="tab-content">
        <% for(var i in tags){ %>
        <%  var val = tags[i],active = i==0?'active':''; %>
            <div class='<%= "tab-pane "+active %>' data-value=<%= val.value %> data-loaded=false  id=<%= val.id %>  > 正在加载中... </div>
        <% } %>
    </div>
</div>